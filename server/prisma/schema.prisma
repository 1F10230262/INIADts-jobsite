datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  label     String
  done      Boolean
  createdAt DateTime
}

model Member {
  githubId     String   @id
  displayName  String
  realName     String
  avatarUrl    String?
  graduateYear Int?
  introduction String?
  links        Json?
  products     Json?
  createdAt    DateTime
}
